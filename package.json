{
  "name": "@qwickapps/auth-client",
  "version": "1.2.0",
  "description": "React authentication components and provider logic for QwickApps - complete frontend auth solution",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.esm.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:dev": "NODE_ENV=development rollup -c",
    "build:prod": "NODE_ENV=production STRIP_LOGS=true rollup -c",
    "dev": "rollup -c -w",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf dist node_modules .rollup.cache storybook-static",
    "prepublishOnly": "npm run test && npm run build",
    "storybook": "storybook dev -p 6008",
    "build-storybook": "storybook build",
    "deploy:dev": "npm run build:dev",
    "deploy": "npm run build:prod"
  },
  "keywords": [
    "authentication",
    "frontend",
    "react",
    "auth",
    "client",
    "supabase",
    "components",
    "ui",
    "qwickapps"
  ],
  "author": "QwickApps",
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "@qwickapps/logging": "^1.0.2",
    "@qwickapps/auth": "^1.0.0",
    "@qwickapps/react-framework": "^1.5.13",
    "@supabase/supabase-js": "^2.54.0",
    "supertokens-web-js": "^0.15.0"
  },
  "peerDependencies": {
    "@emotion/react": ">=11.0.0",
    "@emotion/styled": ">=11.0.0",
    "@mui/icons-material": ">=5.0.0",
    "@mui/material": ">=5.0.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "react-router-dom": ">=6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/preset-typescript": "^7.27.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "babel-jest": "^29.0.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "rollup": "^3.0.0",
    "ts-jest": "^29.4.1",
    "typescript": "^5.9.2",
    "@chromatic-com/storybook": "^4.1.0",
    "@storybook/addon-docs": "^9.1.0",
    "@storybook/react-vite": "^9.1.0",
    "@storybook/react-webpack5": "^9.1.0",
    "storybook": "^9.1.0",
    "css-loader": "^7.1.2",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.2",
    "vite": "^5.0.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.30.1"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest",
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qwickapps/auth-client.git"
  }
}
